{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.10.4",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/lib/fontawesome-iconpicker/",
      "files": [
        "js/fontawesome-iconpicker.min.js",
        "css/fontawesome-iconpicker.css",
        "css/fontawesome-iconpicker.min.css",
        "js/fontawesome-iconpicker.js"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    }
  ]
}